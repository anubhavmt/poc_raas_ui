                },
                {
                  "value": "Starting",
                  "text": "STARTING"
                }
              ]
            }
          }
        },
        "SomePercentageTypeAttribute": {
          "attribute": "SomePercentageTypeAttribute",
          "attributeType": "DOUBLE",
          "object": {
            "apiUrl": "/get-rule-object/<CONTEXT>/<OBJECT_TYPE>/<ATTRIBUTE>/"
          },
          "validator": {
            "operator": [
              "EQ",
              "GT",
              "LT"
            ],
            "value": {
              "Type": "NoRange",
              "RangeValues": {
                "min": 0,
                "max": 100,
                "step": 10
              }
            }
          }
        }
      }
    },
    "actions": [
      {
        "actionOn": "MODULE",
        "type": [
          "ADD_TO_MODULE",
